FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      thermophysicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       const;
    thermo          hConst;
    equationOfState rhoConst;
    specie          specie;
    energy          sensibleEnthalpy;
}

mixture
{
    specie
    {
        nMoles          1;
        molWeight       1;
    }
    equationOfState
    {
        rho             1000;
        T0              300;
        beta            3e-03;
    }
    thermodynamics
    {
        Cp              145.75;
        Hf              0;
    }
    transport
    {
        mu              1e-2;
        Pr              0.031;
    }
}


